<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-money-intercommerz</id>
	<api flags="no_browser"/>
	<name>Интеркоммерц</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе на картах и счетах банка Интеркоммерц <a href="http://intercommerz.ru/">http://intercommerz.ru/</a></p>
	<p>Информация берется из API мобильного приложения.</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>md5.js</js>
		<js>enc-base64-min.js</js>
		<js>isimple-SP-digest.js</js>
		<js>extract.js</js>
		<js>nadapter.js</js>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="card_num" name="Номер карты" type="text"/>
		<counter id="card_type" name="Тип карты" type="text"/>
		<counter id="acc_num" name="Номер счета" type="text"/>
		<counter id="acc_type" name="Тип счета" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="actual" name="Баланс актуален на" type="time" format="dd/MM/yyyy H:m"/>
	</counters>
	<keywords></keywords>
	<type>money, bank</type>
	<country>ru</country>
</provider>